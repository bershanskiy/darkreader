{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "lib": [
            "ES2022",
            "DOM"
        ],
        "types": [
            "chrome",
            "node",
            "jest",
            "jest-extended"
        ],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "inlineSourceMap": true
    }
}
